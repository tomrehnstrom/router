  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@typescript-eslint/utils": "^8.10.0"
  },
  "devDependencies": {
    "@typescript-eslint/rule-tester": "^8.10.0",
    "combinate": "^1.1.11",
    "eslint": "^9.13.0"
  },
  "peerDependencies": {
    "eslint": "^8.57.0 || ^9.0.0"
